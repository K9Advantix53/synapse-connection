


<div className='center'>
  <div id='companyName'>
    <%= link_to 'SYNAPSE', projects_path %>
  </div>
  <div id='aboutPage'>
    <div>
      The Connection Between Businesses and Developers
    </div>
    <div class='zero-opacity homeButton'>
      <%= link_to 'About', about_path %>
      <% if !user_signed_in? %>
        || <%= link_to 'Login', new_user_session_path, class: 'homeButton' %>
      <% else %>
        || <%= link_to 'Projects', projects_path, class: 'homeButton' %>
        || <%= link_to 'Search', search_path, class: 'homeButton' %>
      <% end %>
    </div>
  </div>
</div>
